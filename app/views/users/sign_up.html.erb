<h1> Sign Up </h1>
<hr/>
<%= form_tag do %>
    <table>
        <tr>
            <td align="right">ID : </td>
            <td> <%= text_field_tag :user_name %></td>
        </tr>
        <tr>
            <td align="right">Password : </td>
            <td><%= password_field_tag :password %></td>
        </tr>
        <tr>
            <td align="right">Confirm Password : </td>
            <td> <%= password_field_tag :password_confirmation %></td>
        </tr>
        <tr>
            <td align="right">Age : </td>
            <td> <%= text_field_tag :age %></td>
        </tr> 
        <tr>
            <td align="right">Discord : </td>
            <td> <%= text_field_tag :discord %></td>
        </tr> 
    </table>
    <%= submit_tag 'Sign Up' %>
<% end %>
    